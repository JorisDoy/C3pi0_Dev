package com.indra.iopen.pojo;

import com.indra.iopen.model.ims.SgdIncidencia;
import com.indra.iopen.model.ims.SgdInstalacion;

public class IncidentLocation {
	private SgdIncidencia sgdIncidencia = null;
	private SgdInstalacion sgdInstalacion = null;

	public IncidentLocation(SgdIncidencia sgdIncidencia, SgdInstalacion sgdInstalacion) {
		this.sgdIncidencia = sgdIncidencia;
		this.sgdInstalacion = sgdInstalacion;
	}

	public IncidentLocation() {
	}

	public SgdIncidencia getSgdIncidencia() {
		return sgdIncidencia;
	}

	public void setSgdIncidencia(SgdIncidencia sgdIncidencia) {
		this.sgdIncidencia = sgdIncidencia;
	}

	public SgdInstalacion getSgdInstalacion() {
		return sgdInstalacion;
	}

	public void setSgdInstalacion(SgdInstalacion sgdInstalacion) {
		this.sgdInstalacion = sgdInstalacion;
	}

}
