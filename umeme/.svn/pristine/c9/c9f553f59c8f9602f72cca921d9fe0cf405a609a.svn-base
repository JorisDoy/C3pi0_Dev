package com.indra.iopen.form;

import java.util.Date;

public class ContractConsumption {

	private Date readingDate;
	private String consumptionType;
	private String meterNumber;
	private String meterMake;

	private String readingType;
	private double currentReading;
	private double usage;
	private double meterConstant;

	public ContractConsumption() {

	}

	public Date getReadingDate() {
		return readingDate;
	}

	public void setReadingDate(Date readingDate) {
		this.readingDate = readingDate;
	}

	public String getConsumptionType() {
		return consumptionType;
	}

	public void setConsumptionType(String consumptionType) {
		this.consumptionType = consumptionType;
	}

	public String getMeterNumber() {
		return meterNumber;
	}

	public void setMeterNumber(String meterNumber) {
		this.meterNumber = meterNumber;
	}

	public String getMeterMake() {
		return meterMake;
	}

	public void setMeterMake(String meterMake) {
		this.meterMake = meterMake;
	}

	public String getReadingType() {
		return readingType;
	}

	public void setReadingType(String readingType) {
		this.readingType = readingType;
	}

	public double getCurrentReading() {
		return currentReading;
	}

	public void setCurrentReading(double currentReading) {
		this.currentReading = currentReading;
	}

	public double getUsage() {
		return usage;
	}

	public void setUsage(double usage) {
		this.usage = usage;
	}

	public double getMeterConstant() {
		return meterConstant;
	}

	public void setMeterConstant(double meterConstant) {
		this.meterConstant = meterConstant;
	}

}
