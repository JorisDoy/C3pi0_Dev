background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, #57575B), color-stop(0.8, #86868A));

/* Webkit (Chrome 11+) */ 
background-image: -webkit-linear-gradient(top, #57575B 0%, #86868A 80%);

/* W3C Markup, IE10 Release Preview */ 
background-image: linear-gradient(to bottom, #57575B 0%, #86868A 80%);
