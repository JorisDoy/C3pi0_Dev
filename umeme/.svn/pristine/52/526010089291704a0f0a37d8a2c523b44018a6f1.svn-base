/* Mobile version
=================================================================================== */
@media (max-width: 1024px) {
	#slider {
    height: 310px;
	}
	.slide-text p.extrabig {
    font-size: 2.1em;
	}
	

}

@media (max-width: 600px) {
	#features{
	  display: none;
	}
	.slide-text{
	  display: none;
	}
	#slider {
	  width: 100%;
	  height: 100px;
	}
	#side-tools{
	  display: none;
	}
	.top-strip{
	  /*display: none;*/
	}
	#logo-img{
	  position: relative;
	}
}
@media (max-width: 767px){
	/*
	#meta, #header, .footer-top-line, .footer-top, .footer-bottom, #front-header, #main, .wrap {
		margin-right:auto;
	}
	*/
}
@media only screen and ( max-width: 40em ) /* 640 */
{
   .navbar .btn-navbar {
	  float: left;
	}
}

@media (max-width: 979px) and (min-width: 768px){

    input,textarea,.uneditable-input, select {
    	width:160px
	}
}
@media (max-width: 1215px) and (min-width: 768px){
	.row-fluid .span9{
		margin-left: 8px;
	}
	.top-strip {
    	margin-right: -20px;
	}

}
@media (max-width: 979px){
	.footer-bottom-left .social-media {
		margin-left: 10px;
		float: none;
	}
}

@media (max-width: 350px){
	.footer-nav li{float:left;clear:both;border-left: none;padding-left: 15px !important;}
	.footer-nav li:first-child{}
	.footer-bottom-right.link-style{border-top: 1px solid #555;padding-top:10px}
	.social-media{padding-left:15px}
	.top-strip-nav li{border-right: none;}
}
@media (max-width: 767px) and (min-width: 481px){
	.form-horizontal .control-label{
		float:none
	}
}

