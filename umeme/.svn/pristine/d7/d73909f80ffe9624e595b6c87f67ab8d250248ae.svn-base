#file.service.local.storage.path=D:\\temp\\fileserver
#file.service.local.storage.path=/iopen_files/fileserver
file.service.local.storage.path=C:\\IOPEN\\iopenfiles
# used for validations
file.application.input.min.files=2
file.application.input.max.files=5
file.application.input.max.file.size=1024288
# allowed file extensions (in MIME types)
file.application.allowed.file.extensions=application/pdf,image/jpg,image/jpeg,image/png
# Mime type separator definition
file.application.allowed.file.separator=/
# Default content class definition
file.application.allowed.file.content.class.default=default
#Max file size error
file.upload.max.file.size=File size could not be more than 1 megabyte