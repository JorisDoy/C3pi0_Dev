package com.indra.iopen.form;

public class ContractNumberYearFilter {
	private Long billContractNumber;
	private String billYear;
	private Integer page;
	private Integer batch;

	public ContractNumberYearFilter() {

	}

	public ContractNumberYearFilter(Long contractNumber, String year, Integer page, Integer batch) {
		super();
		this.billContractNumber = contractNumber;
		this.billYear = year;
		this.page = page;
		this.batch = batch;
	}

	public Long getBillContractNumber() {
		return billContractNumber;
	}

	public void setBillContractNumber(Long contractNumber) {
		this.billContractNumber = contractNumber;
	}

	public String getBillYear() {
		return billYear;
	}

	public void setBillYear(String year) {
		this.billYear = year;
	}

	public Integer getPage() {
		return page;
	}

	public void setPage(Integer page) {
		this.page = page;
	}

	public Integer getBatch() {
		return batch;
	}

	public void setBatch(Integer batch) {
		this.batch = batch;
	}

}
